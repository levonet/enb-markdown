    "mock-enb": "^0.3.6",
    "mock-fs": "^4.2.0"
  },
  "scripts": {
    "prepublish": "npm test",
    "pretest": "npm run lint && npm run fixtures",
    "lint": "$(npm bin)/eslint .",
