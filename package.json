  },
  "homepage": "https://github.com/levonet/enb-markdown",
  "dependencies": {
    "lodash": "^4.17.4",
    "markdown-bemjson": "^3.0.6"
  },
  "devDependencies": {
    "bem-core": "^4.2.0",
    "chai": "^3.5.0",
    "enb": ">= 0.15.0 <2.0.0",
    "enb-bem-techs": "^2.2.2",
    "enb-bemxjst": "^8.6.7",
    "eslint": "^3.19.0",
